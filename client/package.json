{
  "name": "smartsafe",
  "version": "0.0.2",
  "description": "Dropbox file syncing",
  "main": "index.js",
  "scripts": {
    "start": "babel-node src/index.js SERVER=false",
    "startServer": "babel-node src/server.js SERVER=true",
    "testrpc": "babel-node testrpc.js",
    "test": "./node_modules/.bin/mocha --reporter spec --require babel-register --recursive",
    "test-debug": "./node_modules/.bin/mocha --reporter spec --require babel-register --recursive --debug-brk",
    "coverage": "./node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha --check-coverage -- --timeout 5000 -u tdd --recursive",
    "watch": "watchify src/app-index.js -t babelify -o src/public/js/app-bundle.js --debug --verbose"
  },
  "keywords": [
    "node.js",
    "dropbox"
  ],
  "author": "Jaan Lambert",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.6",
    "chokidar": "^1.4.3",
    "lodash": "^4.13.1",
    "node-fetch": "^1.5.3",
    "open": "^0.0.5",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "request": "^2.72.0",
    "web3": "^0.17.0-beta",
    "winston": "git+https://github.com/winstonjs/winston.git"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-istanbul": "^0.11.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "babelify": "^7.3.0",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.2",
    "chalk": "^1.1.3",
    "electron": "^1.4.4",
    "electron-reload": "^1.0.2",
    "enzyme": "^2.5.1",
    "ethereumjs-testrpc": "^2.2.0",
    "istanbul": "^0.4.5",
    "jsdom": "9.8.3",
    "jsdom-global": "2.1.0",
    "mocha": "^3.1.2",
    "react-addons-test-utils": "^15.3.2",
    "solc": "^0.4.2",
    "watchify": "^3.7.0"
  }
}
